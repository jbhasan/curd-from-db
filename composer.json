    "description": "Crud Generator From Database Schema",
    "keywords": ["crud", "generator", "generate", "schema", "database"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Hasan Sayeed",
            "email": "jbhasan@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
